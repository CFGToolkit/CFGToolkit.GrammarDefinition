<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Marcin Gołębiowski</Copyright>
    <Description>A .NET library that allows to import, define and transform context free grammars</Description>
    <PackageProjectUrl>https://github.com/CFGToolkit/CFGToolkit.GrammarDefinition</PackageProjectUrl>
    <RepositoryType>GIT</RepositoryType>
    <RepositoryUrl>https://github.com/CFGToolkit/CFGToolkit.GrammarDefinition</RepositoryUrl>
    <PackageTags>parser csharp grammar cfg grammar-parser</PackageTags>
    <Authors>Marcin Gołębiowski</Authors>
    <Company>CFGToolkit</Company>
    <Version>1.0.0-alpha-04</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CFGToolkit.ParserCombinator" Version="1.0.0-beta-16" />
  </ItemGroup>

</Project>
